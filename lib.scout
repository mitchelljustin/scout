module util
    fn range(start, ending) = do
        array = []
        i = start
        while i < ending
            array = array + [i]
            i = i + 1
        end
        array
    end

    fn repeat(string, times) = do
        result = ""
        for _ in util::range(0, times)
            result = result + string
        end
        result
    end

    fn zero() = 0
end

