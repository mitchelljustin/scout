module util
    fn range(start, ending) = do
        array = []
        i = start
        while i < ending
            array = array + [i]
            i = i + 1
        end
        array
    end
end

for i in util::range(0, 100)
    std::print(i, "")
    if i > 10
        break
    end
end
std::println()
std::println("out of loop")
break
